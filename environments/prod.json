{
  "name": "prod",
  "description": "the EC2 single-node production environment",
  "cookbook_versions": {
    "apt":             "= 1.10.0",
    "aws":             "= 0.100.4",
    "bluepill":        "= 2.3.1",
    "build-essential": "= 1.3.2",
    "chef-client":     "= 2.1.6",
    "chef_handler":    "= 1.1.4",
    "cpan":            "= 0.0.29",
    "database":        "= 1.3.10",
    "deploy_wrapper":  "= 0.0.4",
    "dmg":             "= 1.1.0",
    "erlang":          "= 1.1.2",
    "git":             "= 2.1.2",
    "logrotate":       "= 1.0.2",
    "mysql":           "= 2.1.0",
    "nginx":           "= 1.1.2",
    "ohai":            "= 1.1.6",
    "omnibus_updater": "= 1.0.0",
    "openssl":         "= 1.1.0",
    "papertrail":      "= 0.0.3",
    "postgresql":      "= 2.1.0",
    "python":          "= 1.4.4",
    "rsyslog":         "= 1.12.2",
    "runit":           "= 1.4.6",
    "s3cmd":           "= 0.1.1",
    "supervisor":      "= 0.4.12",
    "vim":             "= 1.0.2",
    "windows":         "= 1.7.0",
    "xfs":             "= 1.1.0",
    "yum":             "= 2.1.0",
    "zlib":            "= 2.0.0",
    "vine_shared":     "= 1.0.7",
    "vine_ejabberd":   "= 1.0.5",
    "vine_xmpp":       "= 1.0.7",
    "vine_web":        "= 1.0.3"
  },
  "json_class": "Chef::Environment",
  "chef_type": "environment",
  "default_attributes": {
    "omnibus_updater": { "version": "10.32.2" },
    "chef_client": { "interval": "21600" },
    "dirs": {
      "source": "/home/ubuntu/source_dir",
      "log":    "/home/ubuntu/log_dir",
      "other":  "/home/ubuntu/other",
      "ssl":    "/home/ubuntu/ssl"
    },
    "ejabberd":   {"log_dir": "/home/ubuntu/log_dir/ejabberd"},
    "supervisor": {"log_dir": "/home/ubuntu/log_dir/supervisor"},
    "nginx":      {"log_dir": "/home/ubuntu/log_dir/nginx"}
  },
  "override_attributes": {
  }
}
