{
  "name": "prod",
  "description": "the EC2 multi-node production environment",
  "cookbook_versions": {
    "apt":             "= 1.7.0",
    "aws":             "= 0.100.4",
    "build-essential": "= 1.3.2",
    "chef-client":     "= 2.1.6",
    "chef_handler":    "= 1.1.4",
    "cpan":            "= 0.0.24",
    "database":        "= 1.3.10",
    "deploy_wrapper":  "= 0.0.2",
    "dmg":             "= 1.1.0",
    "erlang":          "= 1.1.2",
    "git":             "= 2.1.2",
    "mysql":           "= 2.1.0",
    "nginx":           "= 1.1.2",
    "ohai":            "= 1.1.6",
    "omnibus_updater": "= 0.1.0",
    "openssl":         "= 1.0.0",
    "postgresql":      "= 2.1.0",
    "python":          "= 1.2.1",
    "runit":           "= 0.16.2",
    "supervisor":      "= 0.4.0",
    "vim":             "= 1.0.2",
    "windows":         "= 1.7.0",
    "xfs":             "= 1.1.0",
    "yum":             "= 2.1.0",
    "vine_shared":     "= 1.0.0",
    "vine_ejabberd":   "= 1.0.0",
    "vine_xmpp":       "= 1.0.0",
    "vine_web":        "= 1.0.0"
  },
  "json_class": "Chef::Environment",
  "chef_type": "environment",
  "default_attributes": {
    "omnibus_updater": { "version": "10.16.4" },
    "dirs": {
      "source": "/home/ec2-user/source_dir",
      "log":    "/home/ec2-user/log_dir",
      "other":  "/home/ec2-user/other",
      "ssl":    "/home/ec2-user/ssl"
    },
    "ejabberd":   {"log_dir": "/home/ec2-user/log_dir/ejabberd"},
    "supervisor": {"log_dir": "/home/ec2-user/log_dir/supervisor"},
    "nginx":      {"log_dir": "/home/ec2-user/log_dir/nginx"}
  },
  "override_attributes": {
  }
}
